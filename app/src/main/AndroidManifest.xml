<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.edu.fafu.se3166016001.butterflyclassify">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name=".AndroidApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Start.StartActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Home.activity.ItemDetail" />
        <activity android:name=".Others.Version" />
        <activity android:name=".Others.AboutUS" />
        <activity android:name=".Others.History" />
        <activity android:name=".Others.UserEdit" />
        <activity android:name=".Home.activity.UpdateInsectInfo" />
        <activity android:name=".Function.activity.InsectClassify" />
        <activity android:name=".Function.activity.AddInsectInfo" />
        <activity
            android:name=".Start.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".Start.RigisterActivity" />
        <activity android:name=".Function.activity.Playground" />
        <activity android:name=".Function.activity.PlaygroundItemDetail" />
        <activity android:name=".Function.activity.ClassifyDetailActivity"></activity>
    </application>

</manifest>